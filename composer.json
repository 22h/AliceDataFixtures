{
    "name": "theofidry/alice-persistence",
    "description": "Nelmio alice extension to persist the loaded fixtures",
    "keywords": ["Fixture", "ORM", "Alice", "Faker", "data", "tests"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Th√©o FIDRY",
            "email": "theo.fidry@gmail.com",
            "homepage": "https://github.com/theofidry"
        }
    ],
    "require": {
        "php": "^7.0",
        "nelmio/alice": "^3.0@dev"
    },
    "require-dev": {
        "doctrine/orm": "^2.2",
        "bamarni/composer-bin-plugin": "dev-master",
        "phpunit/phpunit": "^5.5",
        "doctrine/data-fixtures": "^1.2",
        "symfony/framework-bundle": "^3.1",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/expression-language": "^3.1"
    },
    "suggest": {
        "doctrine/orm": "To use Doctrine ORM"
    },
    "scripts": {
        "post-install-cmd": ["@composer bin all install --ansi"]
    },
    "autoload": {
        "psr-4": { "Fidry\\AliceDataFixtures\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidry\\AliceDataFixtures\\": [
                "fixtures",
                "tests"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "bin-dir": "bin",
        "sort-packages": true
    }
}
